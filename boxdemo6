class box{
	double width,w;
	double height,h;
	double depth,d;
	box(double width,double h,double d){
	this.width=width;
	this.height=h;
	this.depth=d;

	}
	double volume()
{
	return width*height*depth;

}
}
class boxdemo6
{public static void main(String args[])
{doublevol;
box box3=new box(12,34,67);
/
vol=bo3.volume();
System.out.println(vol);
}
}